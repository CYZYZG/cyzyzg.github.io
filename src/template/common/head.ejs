<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="author" content="<%= config.author %>" />
<meta name="keywords" content="<%= config.keywords.join(',') %>" />

<!-- open graph -->
<meta property="og:type" content="website" />
<meta property="og:image" content="/logo.png" />

<link rel="shortcut icon" type="image/png" href="/logo.png" />
<link rel="sitemap" type="application/xml" href="<%= config.publicPath %>/sitemap.xml" />

<link rel="preload" href="<%= config.publicPath %>/<%= titleFontPath %>" as="font" crossOrigin="anonymous" />
<link rel="preload" href="<%= config.publicPath %>/<%= commonFontPath %>" as="font" crossOrigin="anonymous" />
